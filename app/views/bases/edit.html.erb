<h1>拠点情報の更新</h1>
<div class="col-md-6 col-md-offset-3">
  <%= form_with(model: @base, local: true) do |f| %>
    <%= render 'shared/form_error', object: @base %>
    
    <%= f.label "拠点番号" %>
    <%= f.number_field :base_num, class: "form-control" %>
    
    <%= f.label "拠点名" %>
    <%= f.text_field :base_name, class: "form-control" %>
    
    <%= f.label "拠点種類" %>
    <%= f.text_field :kinds, class: "form-control" %>
    
    
    <%= f.submit "更新", class: "btn btn-primary btn-block" %>
  <% end %>
</div>